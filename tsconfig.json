{
  "compilerOptions": {
    "module": "CommonJS",
    "target": "ES6",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "strict": false,
    "outDir": "./dist",
    "declaration": true,
    "jsx": "preserve",
    "paths": {
      "@/*": ["src/*"],
      "vite-mdx":["src/index.ts"],
      "vite-mdx/vue3":["src/vue3/index.ts"],
    }
  },
  "include": ["src","demo/**/*.tsx","demo/**/*.ts","demo/App.tsx"],
  
}
